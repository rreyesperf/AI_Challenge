# Production Environment Configuration Template
# Copy this file to .env.production and configure for your environment

# Authentication Configuration
REACT_APP_AUTH_USERNAME=your_username_here
REACT_APP_AUTH_PASSWORD=your_password_here

# API Configuration
REACT_APP_API_URL=https://your-backend-api.azurecontainerapps.io

# Application Configuration
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.0.0

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=build
