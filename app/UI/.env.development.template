# Development Environment Configuration Template
# For local development: copy this file to .env.local
# For development builds: copy this file to .env.development

# Authentication Configuration (Required)
REACT_APP_AUTH_USERNAME=your_username_here
REACT_APP_AUTH_PASSWORD=your_password_here

# API Configuration
REACT_APP_API_URL=http://localhost:8080

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0

# Development Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_ENABLE_DEVTOOLS=true

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false

# Build Configuration
GENERATE_SOURCEMAP=true
